{
  "name": "html-loader",
  "version": "4.2.0",
  "description": "Html loader module for webpack",
  "license": "MIT",
  "repository": "webpack-contrib/html-loader",
  "author": "Tobias Koppers @sokra",
  "homepage": "https://github.com/webpack-contrib/html-loader",
  "bugs": "https://github.com/webpack-contrib/html-loader/issues",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/webpack"
  },
  "main": "dist/cjs.js",
  "engines": {
    "node": ">= 14.15.0"
  },
  "scripts": {
    "start": "npm run build -- -w",
    "clean": "del-cli dist",
    "validate:runtime": "es-check es5 \"dist/runtime/**/*.js\"",
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production babel src -d dist --copy-files",
    "postbuild": "npm run validate:runtime",
    "commitlint": "commitlint --from=master",
    "security": "npm audit --production",
    "lint:prettier": "prettier --list-different .",
    "lint:js": "eslint --cache .",
    "lint:spelling": "cspell \"**/*.*\"",
    "lint": "npm-run-all -l -p \"lint:**\"",
    "test:only": "cross-env NODE_ENV=test jest",
    "test:watch": "npm run test:only -- --watch",
    "test:coverage": "npm run test:only -- --coverage",
    "pretest": "npm run lint",
    "test": "npm run test:coverage",
    "prepare": "husky install && npm run build",
    "release": "standard-version"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "html-minifier-terser": "^7.1.0",
    "parse5": "^7.1.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.20.7",
    "@babel/core": "^7.20.7",
    "@babel/preset-env": "^7.20.2",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@webpack-contrib/eslint-config-webpack": "^3.0.0",
    "babel-jest": "^29.3.1",
    "cross-env": "^7.0.3",
    "cspell": "^6.18.1",
    "del": "^6.1.1",
    "del-cli": "^4.0.0",
    "es-check": "^7.0.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "handlebars": "^4.7.7",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "memfs": "^3.4.12",
    "npm-run-all": "^4.1.5",
    "posthtml": "^0.16.6",
    "posthtml-webp": "^2.2.0",
    "prettier": "^2.8.1",
    "standard-version": "^9.5.0",
    "unescape-unicode": "^0.2.0",
    "webpack": "^5.75.0"
  },
  "keywords": [
    "webpack",
    "html",
    "loader"
  ]
}
